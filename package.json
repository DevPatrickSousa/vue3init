{
  "name": "vue3-init",
  "displayName": "vue3-init",
  "description": "create vue 3 templates with just one word.",
  "version": "0.0.6",
  "publisher": "vue3-init",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "icon": "./images/icon.jpg",
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "vue3init.createVue3Template",
      "title": "vue3 init"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevPatrickSousa/vue3init.git"
  },
  "changelog": "https://github.com/DevPatrickSousa/vue3init/blob/main/CHANGELOG.md",
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
